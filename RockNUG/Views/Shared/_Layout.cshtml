<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>RockNUG - @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.6.4.min.js")" type="text/javascript"></script>
</head>

<body>
    <div style="width:768px; margin: 0px auto;">
        <div class="header">
            <div style="float:left">
                <p>
                    <img alt="Rockville" src="http://rocknug.org/images/logo.gif"/>
                </p>
                <h2>Rockville's premiere .NET User Group</h2>
                <h2>Meets every 2<sup>nd</sup> Wednesday of the month</h2>
            </div>
            <div class="nav">
                <div class="row">
                    @Html.ActionLink("Directions", "Directions", "Home")
                    @Html.ActionLink("Community", "Community", "Home")
                </div>
                <div class="row">
                    @Html.ActionLink("Home", "Index", "Home")
                    @Html.ActionLink("FAQ", "FAQ", "Home")
                </div>
            </div>
        </div>
        <div class="main">
        @RenderBody()
        </div>
    </div>
    <div class="footer">
    </div>
</body>
</html>
