@model Mvc4Demo.Models.NewsModel
@{
    ViewBag.Title = "Index";
}

<div style="float:left; width: 50%">
<h2>Google News</h2>
<ul>
@foreach(var googleNews in Model.GoogleNews)
{
    <li>
        <a href="@googleNews.Link">@googleNews.Title</a>
    </li>
}
</ul>
</div>
<div style="float:right; width: 50%">
<h2>Yahoo News</h2>
<ul>
@foreach(var yahooNews in Model.YahooNews)
{
    <li>
        <a href="@yahooNews.Link">@yahooNews.Title</a>
    </li>
}
</ul>
</div>
